<!--miniprogram/page/disable/disableRelease/disableRelease.wxml-->
<!-- 标题 -->
<view class="title">
  <view class="title-content" bindtap="goRelease">发布</view>
</view>

<!-- 标题下面的图片 -->
<view class="bg-img">
  <img src="" />
</view>

<!-- 调样式，先顶下去 -->
<view class="sup"></view>

<!-- 下面的一些功能 -->
<view class="funcs">
  <view class="every-icon">
    <text class="iconfont icon-gouwu" id="func-icon"></text>
    <view class="icon-words">商品图片</view>
  </view>
  <view class="every-icon">
    <text class="iconfont icon-favorite" id="func-icon"></text>
    <view class="icon-words">商品名称</view>
  </view>
  <view class="every-icon">
    <text class="iconfont icon-jiagegongshi" id="func-icon"></text>
    <view class="icon-words">商品价格</view>
  </view>
  <view class="every-icon">
    <text class="iconfont icon-chanpinshuliang" id="func-icon"></text>
    <view class="icon-words">商品数量</view>
  </view>
  <view class="every-icon">
    <text class="iconfont icon-miaoshu" id="func-icon"></text>
    <view class="icon-words">商品操作</view>
  </view>
</view>

<!-- 分割线 -->
<view class="dividing "></view>

<!-- 顶下去 -->
<view class="sup-view"></view>

<!-- 下面的活动 -->
<view class="all-activitied" wx:for="{{works}}" wx:key= "works">
  <view class="the-describe">
    <image src="{{item.content}}"></image>
    <view>{{item.name}}</view>
    <view>￥{{item.price}}</view>
    <view>{{item.quantity}}个</view>
    <view class="appointment-func-false" bindtap="goChange" data-id="{{item}}">修改</view>
  </view>
</view>

<!-- 顶上去 -->
<view class="bottom-sup"></view>

<!-- 下面的tabbar -->
<view class="navi-bar">
  <view class="one-bar" bindtap="goHome">
    <view>
      <text class="iconfont icon-home" id="icon-bar"></text>
    </view>
    <view>首页</view>
  </view>
  <view class="one-bar"  id="is-select">
    <view>
      <text class="iconfont icon-fabu" id="icon-bar"></text>
    </view>
    <view>发布</view>
  </view>
  <view class="one-bar" bindtap="goStudy">
    <view>
      <text class="iconfont icon-xinxi" id="icon-bar"></text>
    </view>
    <view>学习</view>
  </view>
  <view class="one-bar" bindtap="goWork">
    <view>
      <text class="iconfont icon-jiuye" id="icon-bar"></text>
    </view>
    <view>就业</view>
  </view>
  <view class="one-bar" bindtap="goMyInfo">
    <view>
      <text class="iconfont icon-wodedangxuan" id="icon-bar"></text>
    </view>
    <view>我的</view>
  </view>
</view>