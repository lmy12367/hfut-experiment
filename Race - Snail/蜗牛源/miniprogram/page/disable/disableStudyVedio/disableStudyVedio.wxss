/* miniprogram/page/disable/disableStudyVedio/disableStudyVedio.wxss */
.title {
  position: fixed;
  background-color: #3a78e7;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  width: 750rpx;
  top: 0;
  z-index: 9999;
  color: #fff;
}

.title-content {
  margin: auto;
  font-size: 35rpx;
  letter-spacing: 3rpx;
}

.sup-view {
  width: 100%;
  height: 160rpx;
}

.search-bg {
  height: 90rpx;
  width: 750rpx;
  background-color: #fff;
  position: fixed;
  left: 10rpx;
  top: 80rpx;
}

.search-view {
  background-color: #f1f1f1;
  display: flex;
  height: 80rpx;
  width: 680rpx;
  line-height: 80rpx;
  margin: auto;
  border-radius: 40rpx;
  padding-left: 30rpx;
}
.vedio-description{
  width: 300rpx;
}
.search-view text {
  font-size: 40rpx;
  color: #c2c2c2;
}

.search-view input {
  margin: auto 0 auto 20rpx;
  font-size: 30rpx;
  width: 500rpx;
}

.input-placeholder {
  letter-spacing: 7rpx;
}

#do-search {
  padding-left: 30rpx;
  font-size: 30rpx;
  color: black;
}

.small-title {
  margin: 20rpx 0 10rpx 20rpx;
}

.vedio-vedio {
  display: flex;
  justify-content: space-evenly;
}


.vedio-vedio image{
  height: 200rpx;
  width: 320rpx;
  border-radius: 10rpx;
}

.right-vedio{
  margin-left: 10rpx;
}

.one-vedio {
  text-align: center;
  margin-bottom: 10rpx;
}

.bottom-sup {
  height: 120rpx;
}

.navi-bar {
  background-color: #fff;
  position: fixed;
  width: 750rpx;
  bottom: 0rpx;
  display: flex;
  justify-content: space-evenly;
  z-index: 9999;
  padding-top: 20rpx;
}

.one-bar {
  color: #999999;
  text-align: center;
}

#icon-img {
  font-size: 40rpx;
  margin-right: 20rpx;
}

#is-select {
  color: rgb(92, 158, 233);
}

#icon-bar {
  font-size: 40rpx;
}