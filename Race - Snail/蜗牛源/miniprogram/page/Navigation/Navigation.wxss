/* miniprogram/page/Navigation/Navigation.wxss */
.navigation-view {
  width: 100%;
  height: 900rpx;
  padding-top: 0;
}
.navigation-view image{
  width: 100%;
  height: 100%;
  position:fixed; 
  background-size:100% 100%;
  z-index: -1;
}
.navigation-view .top{
  margin-top: 200rpx;
}
.navigation-view > view {
  height: 100rpx;
  width: 300rpx;
  margin: auto;
  text-align: center;
  line-height: 100rpx;
  margin-top: 100rpx;
  background-color: #999999;
  color: #ffffff;
}