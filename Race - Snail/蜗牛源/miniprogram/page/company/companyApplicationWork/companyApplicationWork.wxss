/* miniprogram/page/company/companyApplicationWork/companyApplicationWork.wxss */
.title {
  position: fixed;
  background-color: #fff;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  width: 750rpx;
  top: 0;
  z-index: 9999;
}

.title-content {
  margin: auto;
  font-size: 35rpx;
  letter-spacing: 3rpx;
}

#go-back {
  position: fixed;
  top: 0;
  height: 60rpx;
  width: 60rpx;
  font-size: 40rpx;
  font-weight: bold;
}

.sup-view {
  width: 100%;
  height: 80rpx;
}

.ticket-container {
  height: 100%;
  min-height: 100%;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}
 
.title {
  width: 100%;
  height: 88rpx;
  background: white;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
 
.title-sel {
  color: #24272c;
  font-size: 32rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
 
.title-sel  .line-style{
  background: #fff;
  height: 6rpx;
  width: 40rpx;
  position: relative;
  margin-top: 10rpx;
}
 
.title-sel-selected{
  font-size: 32rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #96b9eb;
}
.title-sel-selected .line-style{
  background: #96b9eb;
  height: 2rpx;
  width: 150rpx;
  position: relative;
  margin-top: 10rpx;
}
 
.swiper {
  width: 100%;
  height: 1120rpx;
  flex: 1;
  overflow: scroll;
  margin: 0 auto;
}
 
.record-item {
  margin-top: 10rpx;
  background-color: white;
  padding: 20rpx;
  font-size: 25rpx;
}

.notice-title {
  display: flex;
  justify-content: space-between;
}

.notice-content {
  font-weight: bold;
  letter-spacing: 4rpx;
}

.notice-time {
  color: #999999;
}

.company-identity {
  margin-top: 25rpx;
}

.company-name {
  color: #999999;
  margin-top: 20rpx;
}

.bottom-info {
  color: #999999;
  display: flex;
  justify-content: space-between;
  margin-top: 20rpx;
}
