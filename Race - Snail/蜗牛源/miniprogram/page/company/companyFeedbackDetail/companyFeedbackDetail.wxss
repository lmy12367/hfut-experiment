/* miniprogram/page/company/companyFeedbackDetail/companyFeedbackDetail.wxss */
page {
  background-color: #f6f6f6;
}

.title {
  position: fixed;
  background-color: #fff;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  width: 750rpx;
  top: 0;
  z-index: 9999;
}

.title-content {
  margin: auto;
  font-size: 35rpx;
  letter-spacing: 3rpx;
}

#go-back {
  position: fixed;
  top: 0;
  height: 60rpx;
  width: 60rpx;
  font-size: 40rpx;
  font-weight: bold;
}

.sup-view {
  width: 100%;
  height: 80rpx;
}

.feedback-img {
  width: 100rpx;
  height: 100rpx;
  border-radius: 100rpx;
  background-color: #fff;
  text-align: center;
  line-height: 100rpx;
  position: fixed;
  left: 320rpx;
  top: 130rpx;
}

#pass-icon {
  font-size: 85rpx;
  color: #00a0e9;
}

#nopass-icon {
  font-size: 85rpx;
  color: #ff5771;
}

.dividing {
  width: 100%;
  height: 5rpx;
  background-color: #eae9ea;
}

.notice-content {
  width: 650rpx;
  height: 700rpx;
  padding: 60rpx 10rpx 10rpx 10rpx;
  margin: 100rpx auto 50rpx auto;
  background-color: #fff;
}

.notice-title {
  text-align: center;
  font-size: 30rpx;
}

.feedback-result {
  font-weight: bold;
  letter-spacing: 5rpx;
  margin-bottom: 10rpx;
}

.recieve-time {
  color: #c6c6c6;
}

.company-identity {
  font-weight: bold;
  margin-bottom: 20rpx;
}

.company-name {
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
  color: #999999;
}

.apply-time {
  font-size: 30rpx;
  color: #999999;
  margin: 10rpx 0 35rpx 0;
}

.interview-time {
  margin: 35rpx 0 10rpx 0;
  font-weight: bold;
  letter-spacing: 3rpx;
  font-size: 30rpx;
}

.interview-local {
  margin: 10rpx 0;
  font-weight: bold;
  letter-spacing: 3rpx;
  font-size: 30rpx;
}

.interview-person {
  margin:10rpx 0 35rpx 0 ;
  font-weight: bold;
  letter-spacing: 3rpx;
  font-size: 30rpx;
}

.failed-info {
  margin: 35rpx 0;
  letter-spacing: 5rpx;
  font-size: 28rpx;
  line-height: 50rpx;
}

.failed-name {
  color: #00a0e9;
  font-size: 35rpx;
  margin-right: 15rpx;
}

.remind-title {
  margin: 30rpx 0 15rpx 0;
  color: #999999;
  letter-spacing: 5rpx;
  font-size: 30rpx;
}

.remind-content {
  white-space: pre-wrap;
  font-size: 25rpx;
  letter-spacing: 3rpx;
  color: #999999;
  line-height: 45rpx;
}

.interview-helper {
  color: #00a0e9;
}

.bottom-func {
  width: 650rpx;
  height: 100rpx;
  margin: 0 auto;
  text-align: center;
  line-height: 100rpx;
  background-color: #00a0e9;
  color: #fff;
  border-radius: 10rpx;
}
