<!--miniprogram/page/company/companyReportList/companyReportList.wxml-->
<!-- 标题 -->
<view class="title">
  <text class="title-content">报名名单</text>
</view>

<!-- 顶下去 -->
<view class="sup-view"></view>

<!-- 选择框 -->
<view class='top-selected'>
  <view bindtap='bindShowMsg_local' class="sup-select">
    <text bindtap='bindShowMsg_local'>请选择就业类型</text>
    <text>{{work_type.type_selected}}<text class="iconfont icon-lower-triangle"></text></text>
  </view>
  <!-- 下拉需要显示的列表 -->
  <view class="select-box" wx:if="{{work_type.select}}">
    <view wx:for="{{work_type.types}}" wx:key="unique">
      <view class="select_one" bindtap="mySelect" data-name="{{item}}" data-which="{{work_type.select_me}}"><text>{{item}}</text></view>
    </view>
  </view>
</view>

<!-- 分割线 -->
<view class="dividing "></view>

<!-- 个人的一些信息展示 -->
<!-- 顶上的 -->
<view class="top-info">
  <view class="head-img">
    <img src="" />
  </view>
  <view class="per-info-box">
    <view class="per-info">
      <view class="per-name">小王</view>
      <view class="per-work">景观设计</view>
    </view>
    <view class="per-education">清华大学研究生</view>
  </view>
  <view class="dou-func">
    <text class="iconfont icon-xinaixin3">心愿</text>
    <text class="iconfont icon--chuanshu">私信</text>
  </view>
</view>

<!-- 简单描述 -->
<view class="center-intro">擅长：节选自《节选自《节选自《节选》》》</view>

<!-- 自述 -->
<view class="detail-intro">擅长：节选自《节选自《节选自《节选》》
擅长：节选自《节选自《节选自《节选》》》
擅长：节选自《节选自《节选自《节选》》》
擅长：节选自《节选自《节选自《节选》》》
</view>

<!-- boss回复 -->
<view class="boss-reply">
  <view class="boss-reply-title">boss回复</view>
  <view class="boss-reply-content">这是回复内容</view>
</view>

<!-- 底部选择 -->
<view class="qualified">
  <view class="is-pass" bindtap="doPass">通过</view>
  <view class="boundary"></view>
  <view class="is-pass" bindtap="noPass">不合格</view>
</view>

<!-- 顶上去的 -->
<view class="bottom-sup"></view>