/* miniprogram/page/normal/normalShopping/normalShopping.wxss */
.title {
  position: fixed;
  background-color: #6c79fe;
  color: white;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  width: 750rpx;
  top: 0;
  z-index: 9999;
}

.title-content {
  margin: auto;
}

.number{
  margin-right: 10rpx;
}

.go-back {
  position: fixed;
  top: 0;
  height: 80rpx;
  width: 80rpx;
}

#go-back {
  font-size: 60rpx;
}

.sup {
  width: 100%;
  height: 80rpx;
}

.dividing {
  width: 100%;
  height: 5rpx;
  background-color: #eae9ea;
}

.over-dividing {
  width: 100%;
  height: 25rpx;
  background-color: #eae9ea;
}

.business {
  height: 70rpx;
  line-height: 70rpx;
  color: #6971fe;
  letter-spacing: 3rpx;
  padding-left: 30rpx;
  letter-spacing: 3rpx;
}

.price {
  color: #fe9b00;
}

.one-buss {
  width: 100%;
  display: flex;
  justify-content: space-between;
}

checkbox .wx-checkbox-input {
	width: 34rpx;
	height: 34rpx;
	border-radius: 50%;
}
/*checkbox选中后样式  */
checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  background: #686ffe;
  border-color:#686ffe;
}
/*checkbox选中后图标样式  */
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  width: 20rpx;
  height: 20rpx;  
  line-height: 20rpx;
  text-align: center;
  font-size: 22rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}

.sup-view {
  height: 100rpx;
  width: 750rpx;
}

.bottom-dividing {
  position: fixed;
  bottom: 100rpx;
  width: 100%;
  height: 25rpx;
  z-index: 9999;
  background-color: #eae9ea;
}

.bottom-line {
  z-index: 9999;
  display: flex;
  justify-content: space-between;
  height: 100rpx;
  width: 750rpx;
  line-height: 90rpx;
  position: fixed;
  bottom: 0;
  background-color: #fff;
}

.all-select {
  padding-left: 15rpx;
}

.right-line {
  display: flex;
}

.show-price {
  margin-right: 25rpx;
}

.check-out {
  height: 100rpx;
  width: 200rpx;
  text-align: center;
  color: white;
  background-color: #6972fe;
}